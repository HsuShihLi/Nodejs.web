    <div class="content-region map">
      <div class="row">
        <div class="col-sm-12">
            <h3><em>Address Information</em></h3>
            <div id="map"></div>
            <script>
              function initMap() {

              var uluru = {lat: 37.7661365, lng: -122.460175};
              var map = new google.maps.Map(document.getElementById('map'), {
              zoom: 4,
              center: uluru
              });
              console.log(map);
              var marker = new google.maps.Marker({
              position: uluru,
              map: map
              });
            }
            </script>
            <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBEf_tBMmXMdbzR17oSWVcxQfDF3z-vO8c&callback=initMap"></script>
        </div>
