<div class="container">
<header class="eightimage">
<h1 class="display-3" style="font-family:Bodoni;">Web Design</h1>

</header>

<article class="col-lg-offset-1 col-sm-offset-1 col-lg-8 col-sm-7 col-lg-push-3 col-sm-push-4">
<div class="contact-info">
  <h1 style="padding:4%;">Contact</h1>
</div> 
  <div class="regions">
    <div class="row">
      <div class="col-sm-8">
        <form class="contact-form row" name=form1 id="form1" action="database.js" method="post" accept-charset="Big5">
         <div class="form-group col-sm-6"> 
          <label for="Name">Name</label>
          <input required type="text" class="form-control" 
          name="Name" id="Name">
        </div>
        <div class="form-group col-sm-6">
          <label for="School">School</label>
          <input type="text" class="form-control" 
          name="School" id="School">
        </div>
        <div class="form-group col-sm-6">
          <label for="email">Email</label>
          <input required type="text" class="form-control"
          name="email" id="txtEmail" onBlur="checkEmail(this.value);"/>
        </div>
        <div class="form-group col-sm-6">
           <label for="Phone">Phone</label>
           <input type="text" class="form-control"
           name="Phone" id="Phone" onChange=checkph(this.value)>
        </div> 
        <div class="form-group col-sm-12">
          <label for="Message">Private Message</label>
          <textarea required class="form-control" id="msg" name="Message"
          rows="5" cols="40"></textarea>
        </div>
        <div class="col-sm-12">  
          <p class="form-message float-sm-left"></p>
          <button type="submit" class="btn float-xs-right" color="#FFF" background-color="red" font-weight="bold" name="submit" onclick="self.location.href='database.js'" >Send</button>  
        </div>
      </form>
    </div>
  </div>
</div>

</article>


