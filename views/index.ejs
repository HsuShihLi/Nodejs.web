<% include header %>

<main>
<body>
<div class="container">
<header class="nineimage">
<h1 class="display-3">Web Design</h1>
</header>


  <title>Hsu's Article</title>
  <link rel="stylesheet" 
    href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">

<body class="container">
  <h2 style="font-family:Bodoni;">
   Feedback Article
  <a href="/new-entry" class="btn btn-primary pull-right">
    Write a review article
  </a>
</h2>
</body>

<% if (entries.length) { %>
  <% entries.forEach(function(entry) { %>
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="text-muted pull-right">
          <%= entry.published %>
        </div>
        <%= entry.title %>
      </div>
      <div class="panel-body">
        <%= entry.content %>
      </div>
    </div>
  <% }) %>
<% } else { %>
  Please give us your thoughts and feedback about this website.<br>
  Thank you and have a wonderful day.
  <a href="/new-entry">Create one!!!</a>
<% } %>

</div>
</main>
</body>

<% include footer %>